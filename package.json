    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "knex": "^0.19.0",
    "mongodb": "^3.2.7",
    "mongojs": "^2.6.0",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "styled-components": "^4.3.2"
